{
  "expo": {
    "name": "Babycry",
    "slug": "Babycry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/template.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2A0134"
